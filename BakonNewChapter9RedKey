local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()
	
	ESP.Players = false
	ESP.Boxes = false
	ESP.Names = true
	ESP:Toggle(true)

ESP:AddObjectListener(Workspace.CurrentMap.Chapter9.Utilities,{
		Name = "Red Key",
		CustomName = "Red Key",
		Color = Color3.fromRGB(255, 0, 4),
		IsEnabled = "RedKey",
	})
	ESP.RedKey = true
